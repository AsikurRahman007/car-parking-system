<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300" prefWidth="400" style="-fx-background-color: #002244;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/20.0.1" fx:controller="car_parking_system.Token_GenetatorController">
    <children>
        <VBox spacing="20" AnchorPane.leftAnchor="20" AnchorPane.rightAnchor="20" AnchorPane.topAnchor="20">
            <children>
                <Label style="-fx-font-size: 18px; -fx-font-weight: bold;" text="Token Generator" textFill="WHITE" />
                <HBox spacing="10">
                    <Label text="Car Number:" textFill="WHITE">
                  <font>
                     <Font name="System Bold Italic" size="12.0" />
                  </font></Label>
                    <TextField fx:id="car_no1" promptText="Enter Car Number" />
                </HBox>
                <HBox spacing="10">
                    <Button fx:id="g_t1" onAction="#SubmitAction" text="Generate Token" />
                    <Button fx:id="d_p_r1" onAction="#ReceiptActionButton" text="Display Receipt" />
                    <Button fx:id="btnBack" onAction="#handleBack" text="Back" />
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
